/****************************************************//* Transitions	          			    *//****************************************************//* ab hier bitte nicht ändern */#tx-charbeitsbeispiele-pi1 .masonry,#tx-charbeitsbeispiele-pi1 .masonry .masonry-brick{  -webkit-transition-duration: 0.7s;     -moz-transition-duration: 0.7s;       -o-transition-duration: 0.7s;          transition-duration: 0.7s;}#tx-charbeitsbeispiele-pi1 .masonry{  -webkit-transition-property: height, width;     -moz-transition-property: height, width;       -o-transition-property: height, width;          transition-property: height, width;}#tx-charbeitsbeispiele-pi1 .masonry .masonry-brick{  -webkit-transition-property: left, right, top;     -moz-transition-property: left, right, top;       -o-transition-property: left, right, top;          transition-property: left, right, top;}/* bis hier bitte nicht ändern *//*****************//* Menu          *//*****************/#tx-charbeitsbeispiele-pi1 .shadow{  -moz-box-shadow: 3px 3px 4px #000;  -webkit-box-shadow: 3px 3px 4px #000;  box-shadow: 3px 3px 4px #000;  /* For IE 8 */  -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000')";  /* For IE 5.5 - 7 */  filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000')";	  position:sticky;}#tx-charbeitsbeispiele-pi1 .footer{  width: 120px;  height: 25px;  border-right-width:1px;  border-right-style:solid;  border-right-color:white;  border-bottom-width:1px;  border-bottom-style:solid;  border-bottom-color:silver;  margin-left:-8px;  padding-left:20px;  padding-top: 10px;  background-color: #ffffff;	}#tx-charbeitsbeispiele-pi1 a.contact:link{  text-decoration: none;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 18px;  font-weight: normal;  color: #7f7f7f;}#tx-charbeitsbeispiele-pi1 .logo{  width: 120px;  height: 50px;  border-top-width:1px;  border-top-style:solid;  border-top-color:white;  border-right-width:1px;  border-right-style:solid;  border-right-color:white;  border-bottom-width:1px;  border-bottom-style:solid;  border-bottom-color:silver;  margin-left:-8px;  margin-bottom:0px;  padding-top:30px;  padding-left:20px;  background-color: #343434;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 20px;  font-weight: bold;  color: #d6d6d6;}#tx-charbeitsbeispiele-pi1 .menu{  display: none;				 /* bitte nicht ändern */  padding: 0px;  margin: 0px;}#tx-charbeitsbeispiele-pi1 ul.menu, ul.menu ul{  padding-left: 2px;			/* bitte nicht ändern */  margin-top:0px;				/* bitte nicht ändern */}#tx-charbeitsbeispiele-pi1 ul.menu li{  margin-left: 0px;			/* bitte nicht ändern */  cursor: pointer;			/* bitte nicht ändern */	}/* li */ #tx-charbeitsbeispiele-pi1 .ref_no{  background-color: #efefef;  list-style:inside circle;  padding-left: 12px;  padding-right: 40px;  padding-top: 6px;  padding-bottom: 6px;  border-bottom-width:1px;  border-bottom-style:solid;  border-bottom-color:silver;  border-right-width:1px;  border-right-style:solid;  border-right-color:white;}#tx-charbeitsbeispiele-pi1 .ref_act{  background-color: #ffffff;  list-style:inside disc;  padding-left: 12px;  padding-right: 40px;  padding-top: 6px;  padding-bottom: 6px;  border-bottom-width:1px;  border-bottom-style:solid;  border-bottom-color:silver;  border-right-width:1px;  border-right-style:solid;  border-right-color:white;}#tx-charbeitsbeispiele-pi1 .ref_hover{  background-color: #ffffff;  list-style:inside circle;  padding-left: 12px;  padding-right: 40px;  padding-top: 6px;  padding-bottom: 6px;  border-bottom-width:1px;  border-bottom-style:solid;  border-bottom-color:silver;  border-right-width:1px;  border-right-style:solid;  border-right-color:white;}/* Link */#tx-charbeitsbeispiele-pi1 a.refid:link,a.refid:visited,a.refid:active{  padding: 2px;  text-decoration: none;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 20px;  font-weight: normal;  color: #7f7f7f;}#tx-charbeitsbeispiele-pi1 a.refid:hover{  padding: 2px;  text-decoration: none;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 20px;  font-weight: normal;  color: #7f7f7f;}/****************************************************//* Referenzen / Arbeitsbeispiele		    *//****************************************************/#tx-charbeitsbeispiele-pi1 .wrapper{  min-width: 1900px;  padding: 0px;  margin: 0px;}#singleview{  position: relative;		  /* bitte nicht ändern */  float: left;		  /* bitte nicht ändern */	  width: 550px;		  /* bitte nicht ändern */  height: 0px;                /* bitte nicht ändern */  background-color: #ffffff;    /* bitte nicht ändern */  margin-left: 15px;		  /* bitte nicht ändern */  margin-top: 0px;		  /* bitte nicht ändern */  margin-bottom: 0px;		  /* bitte nicht ändern */  margin-right: 0px;		  /* bitte nicht ändern */  padding-right: 100px; 		   display: block;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 20px;  font-weight: normal;  color: #000000;  visibility: hidden;}#container{  position: relative;		  /* bitte nicht ändern */  float: left;		  /* bitte nicht ändern */	  width: 1200px;		  /* bitte nicht ändern */  height: 800px;                /* bitte nicht ändern */  background-color: #ffffff;    /* bitte nicht ändern */  margin-left: 180px;		  /* bitte nicht ändern */  margin-top: 0px;		  /* bitte nicht ändern */  margin-bottom: 0px;		  /* bitte nicht ändern */  margin-right: 0px;		  /* bitte nicht ändern */  padding: 0px; 		 /* bitte nicht ändern */  z-index: -100;}/* Gallery-Bild */#tx-charbeitsbeispiele-pi1 .brick{  width: 374px;			/* bitte nicht ändern */		  float: left;			/* bitte nicht ändern */	  overflow: hidden;		/* bitte nicht ändern */  display: none;			/* bitte nicht ändern */	  margin-bottom: 15px;		/* bitte nicht ändern */  padding: 0px;              	   /* bitte nicht ändern */  background-color: #343434;  vertical-align:top;}#tx-charbeitsbeispiele-pi1 .background{  width: 374px;			/* bitte nicht ändern */  background-color:  #343434; }/* Zusammenfassung/Anreißer: Box-Aussehen */#tx-charbeitsbeispiele-pi1 .teaser{  display: none;				 /* bitte nicht ändern */  width: 374px;				/* bitte nicht ändern */  margin: 0px;								  padding-top: 10px;			/* bitte nicht ändern */ /* dieser Wert muß noch zu Höhe des Elementes hinzuaddiert werden */  padding-bottom: 25px;			/* bitte nicht ändern */ /* dieser Wert muß noch zu Höhe des Elementes hinzuaddiert werden */  padding-left: 10px;  background-color: #343434;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 16px;  font-weight: normal;  color: #d6d6d6;}/* Titel in Summary */#tx-charbeitsbeispiele-pi1 .teaser h3{  width: 374px;				/* bitte nicht ändern */}/* Text in Summary */#tx-charbeitsbeispiele-pi1 .teaser p .subheadline {  width: 374px;				/* bitte nicht ändern */}/* Text in Summary */#tx-charbeitsbeispiele-pi1 .teaser p{  width: 334px;				/* bitte nicht ändern */  padding: 0;				  bitte nicht ändern */  margin: 0;				/* bitte nicht ändern */  }#tx-charbeitsbeispiele-pi1 .divider{  width: 354px;				/* bitte nicht ändern */  margin-left: -1px;			  margin-top: 10px;  margin-bottom: 10px;  padding: 0px;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 10px;  font-weight: normal;  color: #626262;}/* Bild */#tx-charbeitsbeispiele-pi1 .img{  padding:0px;		 /* bitte nicht ändern */  margin-top:0px;		/* bitte nicht ändern */}/* Link */#tx-charbeitsbeispiele-pi1 .brick a.arrow:link, a.arrow:hover, a.arrow:visited, a.arrow:active{  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 20px;  font-weight: normal;  color: #d6d6d6;  border: none;  text-decoration: none; }#tx-charbeitsbeispiele-pi1 .singleview a:link, a:visited, a:active{  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 20px;  font-weight: normal;  color: #9f9f9f;  border: none;}#tx-charbeitsbeispiele-pi1 .singleview a:hover{  color: #d6d6d6;}#tx-charbeitsbeispiele-pi1 .singleview a:visited{  color: #343434;}#tx-charbeitsbeispiele-pi1 .singleview a.bb:link, a.bb:visited, a.bb:active{  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 15px;  font-weight: normal;  color: #343434;  border: none;  text-decoration: none;}#tx-charbeitsbeispiele-pi1 .singleview a.bb:hover{  color: #d6d6d6;}#tx-charbeitsbeispiele-pi1 .content{  text-decoration: none;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 12px;  font-weight: normal;  color: #000000;  border: none;}#tx-charbeitsbeispiele-pi1 .singleview #imgtext-table {  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 12px;  font-weight: normal;  color: #000000;  border: none;}#tx-charbeitsbeispiele-pi1 .singleview p{  clear:both;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 15px;  font-weight: normal;  color: #000000;  border: none;  padding-bottom: 10px;}#tx-charbeitsbeispiele-pi1 .singleview ul, li{  clear:both;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 15px;  font-weight: normal;  color: #000000;  border: none;  padding-bottom: 0px;}#tx-charbeitsbeispiele-pi1 table{  padding-right: 7px;  padding-bottom: 7px;}#tx-charbeitsbeispiele-pi1 #smilie{  width:370px;  height:300px;  font-family: Tahoma, Arial, Helvetica, Verdana, Sans-Serif;  font-size: 28px;  font-weight: bold;  color: #d6d6d6;  text-align: left;  padding: 10px;  overflow-x: no-content;  white-space:pre-wrap;}/* CSS code, add in a separate file or style tag *//* always applied */#tx-charbeitsbeispiele-pi1 nav {  width: 100%;  background-color: black;  text-align: center;  margin: 0 auto;}/* applied after scroll height reached */#tx-charbeitsbeispiele-pi1 .fixed-nav {  position: fixed;  top:0;  z-index: 10;}